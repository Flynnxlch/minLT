{
  "name": "minlt-backend",
  "version": "0.2.2x",
  "description": "MinLT Backend Server - Node.js API",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "dev": "node --watch src/server.js",
    "start": "node src/server.js",
    "test": "node --test",
